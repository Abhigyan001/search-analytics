<div>
  <%= link_to 'Analytics', analytics_path %>

  <h1>Search Article</h1>

  <div id="articles-section">
    <%= render partial: '/shared/form' %>
  </div>

  <%= turbo_frame_tag "articles" do %>
    <% if !@articles.empty? %>
      <div class='queries'>
        <% @articles.each do |article| %>
          <ul>
              <li> <a href='/articles/<%= article[1]%>'><%= article[0] %></a></li>
          </ul>
        <% end %>
      </div>
    <% end %>
  <% end %>
</div>
