<div>
  <h1>Search Article</h1>

  <div id="articles-section">
    <%= render partial: '/shared/form' %>
  </div>

  <%= turbo_frame_tag "articles" do %>
    <% if @articles.empty? %>
     <div></div>
    <% else %>
      <% @articles.each do |article| %>
        <ul class="article">
          <li><%= article.title %></li>
        </ul>
      <% end %>
    <% end %>
  <% end %>
</div>
