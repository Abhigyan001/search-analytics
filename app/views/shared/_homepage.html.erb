<div class='search'>
  <h1>Search Articles</h1>
  <%= render partial: '/shared/form' %>

  <%= turbo_frame_tag "articles" do %>
    <% if !articles.empty? %>
      <div>
        <% articles.each do |article| %>
          <ul>
            <li> <a href='/articles/<%= article[1]%>'><%= article[0] %></a> </li>
          </ul>
        <% end %>
      </div>
    <% end %>
  <% end %>
</div>
