<div class='container'>
  <%= render partial: '/shared/form' %>

  <%= turbo_frame_tag "articles" do %>
    <% if !articles.empty? %>
      <div>
        <% articles.each do |article| %>
          <ul class="search-list">
            <li class= "list-item text-left">
              <a href='/articles/<%= article[1]%>'>
                <%= image_tag "search-icon.jpg", class: "list-icon" %>
                <%= article[0] %>
              </a>
            </li>
          </ul>
        <% end %>
      </div>
    <% end %>
  <% end %>
</div>
